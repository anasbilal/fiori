<mvc:View
	controllerName="sap.Bekon.controller.cockpit"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<MessagePage
		title="{i18n>notFoundTitle}"
		text="{i18n>notFoundText}"
		icon="{sap-icon://document}"
		id="page">
		<customDescription>
			<Link id="link" text="{i18n>backToCockpit}" press="onNavPress" />
		</customDescription>
	</MessagePage>
</mvc:View>